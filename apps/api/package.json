{
  "name": "telli-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "start": "NODE_ENV=production tsx src/index.ts",
    "build": "tsup",
    "lint": "tsc --noEmit && eslint --max-warnings 0",
    "types": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "vitest run --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@telli/api-database": "workspace:*",
    "@telli/api-llm-model": "workspace:*",
    "@telli/api-utils": "workspace:*",
    "@fastify/cors": "11.2.0",
    "@fastify/multipart": "9.4.0",
    "@fastify/otel": "0.16.0",
    "@fastify/swagger": "9.7.0",
    "@fastify/swagger-ui": "5.2.5",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.69.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.212.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.212.0",
    "@opentelemetry/resources": "2.5.1",
    "@opentelemetry/sdk-metrics": "2.5.1",
    "@opentelemetry/sdk-node": "0.212.0",
    "@opentelemetry/sdk-trace-node": "2.5.1",
    "@opentelemetry/semantic-conventions": "1.39.0",
    "@sentry/node": "10.38.0",
    "@sentry/opentelemetry": "10.38.0",
    "@sentry/profiling-node": "10.38.0",
    "@t3-oss/env-core": "0.13.10",
    "dotenv": "17.3.1",
    "fastify": "5.7.4",
    "fastify-graceful-shutdown": "5.0.0",
    "google-auth-library": "10.5.0",
    "openai": "6.22.0",
    "openaiv4": "npm:openai@4.104.0",
    "tiktoken": "1.0.22",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@telli/api-eslint-config": "workspace:*",
    "@telli/api-typescript-config": "workspace:*",
    "@sentry/esbuild-plugin": "4.9.1",
    "@types/node": "24.10.13",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.2",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
