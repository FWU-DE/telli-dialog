{
  "name": "telli-dialog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "op": "op run --env-file=\"./.env\" --no-masking",
    "op:signin": "eval $(op signin)",
    "dev": "ENVIRONMENT=development pnpm op -- next dev --turbo",
    "dev:local": "dotenvx run -f .env.local -- next dev --turbo",
    "dev:e2e": "ENVIRONMENT=e2e pnpm op -- next dev --turbo",
    "build": "next build",
    "build:envless": "next build --experimental-build-mode compile",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "start": "next start",
    "start:e2e": "ENVIRONMENT=e2e pnpm op -- next start",
    "start:oidc": "tsx ./e2e/vidis-mock-server.ts",
    "build:oidc": "node e2e/load_test/esbuild.oidc.config.js",
    "lint": "next lint",
    "types": "tsc",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:migrate:local": "dotenvx run -f .env.local -- drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:migrate:op": "ENVIRONMENT=development op run --env-file=\"./.env\" -- drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:seed": "tsx src/db/seed/index.ts",
    "db:seed:op": "ENVIRONMENT=development op run --env-file=\"./.env\" -- pnpm tsx src/db/seed/index.ts",
    "db:seed:local": "ENVIRONMENT=development dotenvx run -f .env.local -- pnpm tsx src/db/seed/index.ts",
    "db:studio": "dotenv -e .env -- drizzle-kit studio --config=src/db/drizzle.config.ts",
    "otc:dump": "dotenv -e .env -- pnpm tsx src/scripts/mirgrate_otc/index.ts",
    "drizzle:up": "drizzle-kit up --config=src/db/drizzle.config.ts",
    "e2e": "dotenv -e .env -- playwright test",
    "e2e:api": "dotenvx run -f .env.local -- playwright test tests/api/",
    "e2e:op": "ENVIRONMENT=e2e pnpm op -- playwright test",
    "e2e:headed": "ENVIRONMENT=e2e pnpm op -- playwright test --headed",
    "e2e:ui": "ENVIRONMENT=e2e pnpm op -- playwright test --ui",
    "k6:build": "node e2e/load_test/esbuild.k6.config.js",
    "k6:run:ui": "K6_BROWSER_HEADLESS=false pnpm k6:build && K6_BROWSER_HEADLESS=false k6 run e2e/load_test/index.js -e K6_BROWSER_HEADLESS=false",
    "k6:run": "pnpm k6:build && K6_BROWSER_HEADLESS=true K6_BROWSER_ARGS='no-sandbox' k6 run e2e/load_test/index.js -e K6_BROWSER_HEADLESS=true -e K6_BROWSER_ARGS='no-sandbox'",
    "start:cluster": "pm2 start e2e/load_test/ecosystem.config.js",
    "stop:cluster": "pm2 stop all",
    "delete:cluster": "pm2 delete all",
    "status:cluster": "pm2 status",
    "logs:cluster": "pm2 logs"
  },
  "packageManager": "pnpm@9.6.0",
  "dependencies": {
    "@ai-sdk/openai": "1.1.9",
    "@ai-sdk/react": "1.2.12",
    "@aws-sdk/client-s3": "3.744.0",
    "@aws-sdk/s3-request-presigner": "3.744.0",
    "@deutschlandgpt/core": "0.1.5",
    "@dotenvx/dotenvx": "1.44.1",
    "@hookform/resolvers": "3.10.0",
    "@mozilla/readability": "0.6.0",
    "@opentelemetry/instrumentation-pg": "0.51.0",
    "@radix-ui/react-alert-dialog": "1.1.6",
    "@radix-ui/react-checkbox": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.6",
    "@radix-ui/react-select": "2.1.6",
    "@radix-ui/react-toast": "1.2.6",
    "@radix-ui/react-tooltip": "1.1.8",
    "@sentry/nextjs": "9.32.0",
    "@t3-oss/env-nextjs": "0.10.1",
    "@tanstack/react-query": "5.66.0",
    "@types/express": "5.0.0",
    "@types/pg": "8.11.11",
    "ai": "4.3.16",
    "bcrypt": "5.1.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "docx": "9.1.1",
    "dotenv-cli": "7.4.1",
    "drizzle-kit": "0.31.1",
    "drizzle-orm": "0.39.3",
    "drizzle-zod": "0.7.1",
    "he": "1.2.0",
    "import-in-the-middle": "1.14.2",
    "ioredis": "5.6.0",
    "jsdom": "24.1.0",
    "k6": "0.0.0",
    "katex": "0.16.21",
    "mammoth": "1.9.0",
    "markdown-it": "14.1.0",
    "nanoid": "3.3.8",
    "next": "15.3.4",
    "next-auth": "5.0.0-beta.25",
    "next-axiom": "1.9.1",
    "next-intl": "3.26.3",
    "next-runtime-env": "3.3.0",
    "openai": "4.83.0",
    "pg": "8.13.1",
    "pm2": "6.0.5",
    "postcss": "8.4.49",
    "qrcode.react": "4.2.0",
    "rabbitmq-client": "5.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-image-crop": "11.0.7",
    "react-markdown": "9.0.3",
    "react-syntax-highlighter": "15.6.1",
    "react-textarea-autosize": "8.5.7",
    "rehype-katex": "7.0.1",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "require-in-the-middle": "7.5.1",
    "sentence-splitter": "^5.0.0",
    "sharp": "0.33.3",
    "tailwind-merge": "3.0.1",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "1.0.7",
    "tsx": "4.19.4",
    "unpdf": "1.0.5",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@playwright/test": "1.50.1",
    "@types/bcrypt": "5.0.2",
    "@types/he": "1.2.0",
    "@types/jsdom": "21.1.7",
    "@types/k6": "0.57.1",
    "@types/katex": "0.16.7",
    "@types/markdown-it": "14.1.2",
    "@types/node": "20.17.12",
    "@types/oidc-provider": "8.8.0",
    "@types/react": "19.0.3",
    "@types/react-dom": "19.0.2",
    "@types/react-syntax-highlighter": "15.5.13",
    "esbuild": "0.25.5",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.3",
    "express": "4.21.2",
    "jose": "6.0.6",
    "oidc-provider": "8.8.0",
    "prettier": "3.4.2",
    "typescript": "5.7.2",
    "vitest": "3.0.5"
  }
}
