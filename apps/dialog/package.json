{
  "name": "telli-dialog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm start:oidc\" \"next dev -p 3000 --turbo\"",
    "build": "next build",
    "build:envless": "next build --experimental-build-mode compile",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "start": "next start",
    "start:oidc": "tsx ./e2e/vidis-mock-server.ts",
    "build:oidc": "node e2e/load_test/esbuild.oidc.config.js",
    "lint": "next lint",
    "types": "tsc",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "dotenvx run -f .env.local -- drizzle-kit generate --config=./drizzle.config.ts",
    "db:drop": "dotenvx run -f .env.local -- drizzle-kit drop --config=./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate:local": "dotenvx run -f .env.local -- drizzle-kit migrate --config=./drizzle.config.ts",
    "db:seed": "tsx src/db/seed/index.ts",
    "db:seed:local": "ENVIRONMENT=development dotenvx run -f .env.local -- pnpm tsx src/db/seed/index.ts",
    "e2e": "NODE_OPTIONS=--experimental-require-module playwright test",
    "e2e:api": "dotenvx run -f .env.local -- playwright test tests/api/",
    "e2e:api:ui": "dotenvx run -f .env.local -- playwright test tests/api/ --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "k6:build": "node e2e/load_test/esbuild.k6.config.mjs"
  },
  "packageManager": "pnpm@9.15.1",
  "dependencies": {
    "@ai-sdk/openai": "1.1.9",
