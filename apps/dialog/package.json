{
  "name": "telli-dialog",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000 --turbo",
    "build": "next build",
    "build:envless": "next build --experimental-build-mode compile",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "start": "next start",
    "lint": "eslint .",
    "types": "tsc",
    "check-types": "next typegen && tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "e2e": "NODE_OPTIONS=--experimental-require-module playwright test",
    "e2e:api": "dotenvx run -f .env.local -- playwright test tests/api/",
    "e2e:api:ui": "dotenvx run -f .env.local -- playwright test tests/api/ --ui",
    "e2e:headed": "dotenvx run -f .env.local -- playwright test --headed",
    "e2e:ui": "dotenvx run -f .env.local -- playwright test --ui",
    "k6:build": "node e2e/load_test/esbuild.k6.config.mjs",
    "prek6:run": "pnpm k6:build",
    "k6:run": "dotenvx run -f .env.k6 -- k6 run",
    "prek6:run:cloud": "pnpm k6:build",
    "k6:run:cloud": "k6 cloud run -e LOADTEST_USERNAME=$LOADTEST_USERNAME -e LOADTEST_PASSWORD=$LOADTEST_PASSWORD -e LOADTEST_IDP_HINT=$LOADTEST_IDP_HINT -e LOADTEST_BASE_URL=$LOADTEST_BASE_URL"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.990.0",
    "@aws-sdk/s3-request-presigner": "3.990.0",
    "@deutschlandgpt/core": "0.1.5",
    "@dotenvx/dotenvx": "1.52.0",
    "@hookform/resolvers": "5.2.2",
    "@mozilla/readability": "0.6.0",
    "@opentelemetry/api": "1.9.0",
    "@phosphor-icons/react": "2.1.10",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-toast": "1.2.15",
    "@radix-ui/react-tooltip": "1.2.8",
    "@sentry/nextjs": "10.38.0",
    "@t3-oss/env-nextjs": "0.13.10",
    "@tanstack/react-query": "5.90.21",
    "@telli/ai-core": "workspace:*",
    "@telli/shared": "workspace:*",
    "@telli/ui": "workspace:*",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "docx": "9.5.2",
    "drizzle-kit": "0.31.9",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "he": "1.2.0",
    "import-in-the-middle": "3.0.0",
    "ioredis": "5.9.3",
    "isbinaryfile": "6.0.0",
    "jsdom": "28.1.0",
    "katex": "0.16.28",
    "mammoth": "1.11.0",
    "markdown-it": "14.1.1",
    "nanoid": "5.1.6",
    "next": "16.1.6",
    "next-auth": "5.0.0-beta.30",
    "next-axiom": "1.10.0",
    "next-intl": "4.8.2",
    "pg": "8.18.0",
    "postcss": "8.5.6",
    "qrcode.react": "4.2.0",
    "rabbitmq-client": "5.0.8",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "react-image-crop": "11.0.10",
    "react-markdown": "10.1.0",
    "react-syntax-highlighter": "16.1.0",
    "react-textarea-autosize": "8.5.9",
    "rehype-katex": "7.0.1",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "require-in-the-middle": "8.0.1",
    "sentence-splitter": "5.0.0",
    "sharp": "0.34.5",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "1.0.7",
    "tsx": "4.21.0",
    "unpdf": "1.4.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@playwright/test": "1.58.2",
    "@telli/typescript-config": "workspace:*",
    "@types/he": "1.2.3",
    "@types/jsdom": "27.0.0",
    "@types/k6": "1.6.0",
    "@types/katex": "0.16.8",
    "@types/markdown-it": "14.1.2",
    "@types/node": "24.10.1",
    "@types/pg": "8.16.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/react-syntax-highlighter": "15.5.13",
    "esbuild": "0.27.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-turbo": "2.8.9",
    "eslint-plugin-prettier": "5.5.5",
    "jose": "6.1.3",
    "k6": "0.0.0",
    "playwright-core": "1.58.2",
    "prettier": "3.8.1",
    "prettier-eslint": "16.4.2",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
