{
  "name": "telli-dialog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:op": "op run --env-file=\"./.env.e2e\" -- next dev --turbo",
    "dev:turbo": "next dev --turbo",
    "dev:ssl": "next dev --experimental-https",
    "build": "next build",
    "build:envless": "next build --experimental-build-mode compile",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "start": "next start",
    "start:op:e2e": "op run --env-file=\"./.env.e2e\" -- next start",
    "start:oidc": "tsx ./e2e/vidis-mock-server.ts",
    "lint": "next lint",
    "types": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:migrate:op": "op run --env-file=\"./.env\" -- drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:migrate:envless": "drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:seed": "op run --env-file=\"./.env\" -- pnpm tsx src/db/seed/index.ts",
    "db:seed:op": "op run --env-file=\"./.env\" -- pnpm tsx src/db/seed/index.ts",
    "db:studio": "dotenv -e .env -- drizzle-kit studio --config=src/db/drizzle.config.ts",
    "drizzle:up": "drizzle-kit up --config=src/db/drizzle.config.ts",
    "e2e": "dotenv -e .env -- playwright test",
    "e2e:headed": "dotenv -e .env -- playwright test --headed",
    "e2e:ui": "dotenv -e .env -- playwright test --ui",
    "postinstall": "playwright install"
  },
  "packageManager": "pnpm@9.6.0",
  "dependencies": {
    "@ai-sdk/openai": "1.1.9",
    "@ai-sdk/react": "1.1.11",
    "@aws-sdk/client-s3": "3.744.0",
    "@aws-sdk/s3-request-presigner": "3.744.0",
    "@deutschlandgpt/core": "0.1.5",
    "@hookform/resolvers": "3.10.0",
    "@opentelemetry/instrumentation-pg": "0.51.0",
    "@radix-ui/react-alert-dialog": "1.1.6",
    "@radix-ui/react-checkbox": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.6",
    "@radix-ui/react-select": "2.1.6",
    "@radix-ui/react-toast": "1.2.6",
    "@sentry/nextjs": "9.0.0",
    "@t3-oss/env-nextjs": "0.10.1",
    "@tanstack/react-query": "5.66.0",
    "@types/express": "^5.0.0",
    "@types/pg": "^8.11.11",
    "ai": "4.1.34",
    "bcrypt": "5.1.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "docx": "9.1.1",
    "drizzle-orm": "0.39.3",
    "import-in-the-middle": "1.13.0",
    "katex": "0.16.21",
    "markdown-it": "14.1.0",
    "nanoid": "5.0.9",
    "next": "15.1.3",
    "next-auth": "5.0.0-beta.25",
    "next-axiom": "1.9.1",
    "next-intl": "3.26.3",
    "next-runtime-env": "3.2.2",
    "openai": "4.83.0",
    "pg": "8.13.1",
    "qrcode.react": "4.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-image-crop": "11.0.7",
    "react-markdown": "9.0.3",
    "react-syntax-highlighter": "15.6.1",
    "react-textarea-autosize": "8.5.7",
    "rehype-katex": "7.0.1",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "require-in-the-middle": "7.5.1",
    "tailwind-merge": "3.0.1",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@playwright/test": "^1.50.1",
    "@types/bcrypt": "5.0.2",
    "@types/katex": "0.16.7",
    "@types/markdown-it": "14.1.2",
    "@types/node": "20.17.12",
    "@types/oidc-provider": "^8.8.0",
    "@types/react": "19.0.3",
    "@types/react-dom": "19.0.2",
    "@types/react-syntax-highlighter": "15.5.13",
    "dotenv-cli": "7.4.1",
    "drizzle-kit": "0.30.4",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.3",
    "express": "^4.21.2",
    "jose": "^6.0.6",
    "oidc-provider": "^8.8.0",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "tailwindcss": "3.4.1",
    "tsx": "4.7.2",
    "typescript": "5.7.2",
    "vitest": "3.0.5"
  }
}
