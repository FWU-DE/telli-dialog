{
  "name": "telli-chatbot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:docker:dialog": "docker build -t telli-dialog -f ./apps/dialog/Dockerfile .",
    "build:docker:admin": "docker build -t telli-admin -f ./apps/admin/Dockerfile .",
    "dev": "turbo run dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "check-types": "turbo run check-types"
  },
  "packageManager": "pnpm@10.24.0",
  "devDependencies": {
    "@telli/typescript-config": "workspace:*",
    "prettier": "3.7.4",
    "turbo": "2.7.2"
  },
  "pnpm": {
    "overrides": {
      "glob@<11.1.0": ">=11.1.0"
    }
  }
}
