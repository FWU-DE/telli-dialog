{
  "name": "@telli/ai-core",
  "version": "0.0.1",
  "main": "src/index.ts",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "db:seed:telli-api": "tsx src/api-db/seed/index.ts",
    "lint": "eslint \"**/*.ts*\" --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "google-auth-library": "10.5.0",
    "js-tiktoken": "^1.0.21",
    "openai": "6.9.1",
    "openaiv4": "npm:openai@4.104.0",
    "pg": "8.18.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@t3-oss/env-core": "0.13.10",
    "@telli/typescript-config": "workspace:*",
    "@types/node": "24.10.1",
    "@types/pg": "8.16.0",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "@vitest/coverage-v8": "4.0.18",
    "bcryptjs": "3.0.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-turbo": "2.8.1",
    "eslint-plugin-prettier": "5.5.5",
    "nanoid": "5.1.6",
    "prettier": "3.8.1",
    "prettier-eslint": "16.4.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
