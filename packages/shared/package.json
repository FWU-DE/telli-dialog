{
  "name": "@telli/shared",
  "version": "0.0.1",
  "main": "src/index.ts",
  "type": "module",
  "private": true,
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "db:generate": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit generate --config=./drizzle.config.ts",
    "db:drop": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit drop --config=./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate:local": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit migrate --config=./drizzle.config.ts",
    "db:seed": "tsx src/db/seed/index.ts",
    "db:seed:local": "ENVIRONMENT=development dotenvx run -f ../../apps/dialog/.env.local -- pnpm tsx src/db/seed/index.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "lint": "eslint \"**/*.ts*\" --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.744.0",
    "@aws-sdk/s3-request-presigner": "3.744.0",
    "@dotenvx/dotenvx": "1.44.1",
    "@t3-oss/env-core": "0.13.8",
    "bcrypt": "5.1.1",
    "drizzle-kit": "0.31.1",
    "drizzle-orm": "0.39.3",
    "drizzle-zod": "0.7.1",
    "nanoid": "5.0.9",
    "pg": "8.13.3",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@telli/typescript-config": "workspace:*",
    "@types/bcrypt": "5.0.2",
    "@types/node": "22.13.14",
    "@types/pg": "8.11.11",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "dotenv-cli": "10.0.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-turbo": "2.5.8",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "3.4.2",
    "prettier-eslint": "16.4.2",
    "typescript": "5.7.2",
    "tsx": "4.19.4",
    "vitest": "3.0.5"
  }
}
