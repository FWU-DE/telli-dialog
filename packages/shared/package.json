{
  "name": "@telli/shared",
  "version": "0.0.1",
  "main": "src/index.ts",
  "type": "module",
  "private": true,
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "db:generate": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit generate --config=./drizzle.config.ts",
    "db:drop": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit drop --config=./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate:local": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit migrate --config=./drizzle.config.ts",
    "db:seed": "tsx src/db/seed/index.ts",
    "db:seed:local": "ENVIRONMENT=development dotenvx run -f ../../apps/dialog/.env.local -- pnpm tsx src/db/seed/index.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "lint": "eslint \"**/*.ts*\" --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.948.0",
    "@aws-sdk/s3-request-presigner": "3.948.0",
    "@dotenvx/dotenvx": "1.51.1",
    "@sentry/nextjs": "10.30.0",
    "@telli/ai-core": "workspace:*",
    "@t3-oss/env-core": "0.13.8",
    "drizzle-kit": "0.31.8",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "nanoid": "5.1.6",
    "next": "15.5.9",
    "pg": "8.16.3",
    "unstorage": "1.17.3",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.1",
    "@telli/typescript-config": "workspace:*",
    "@types/node": "24.10.1",
    "@types/pg": "8.16.0",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "@vitest/coverage-v8": "4.0.15",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-turbo": "2.6.3",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "3.7.4",
    "prettier-eslint": "16.4.2",
    "typescript": "5.9.3",
    "tsx": "4.21.0",
    "vitest": "4.0.15"
  }
}
