{
  "name": "@telli/shared",
  "version": "0.0.1",
  "main": "src/index.ts",
  "type": "module",
  "private": true,
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "db:generate": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit generate --config=./drizzle.config.ts",
    "db:drop": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit drop --config=./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate:local": "dotenvx run -f ../../apps/dialog/.env.local -- drizzle-kit migrate --config=./drizzle.config.ts",
    "db:seed": "tsx src/db/seed/index.ts",
    "db:seed:local": "ENVIRONMENT=development dotenvx run -f ../../apps/dialog/.env.local -- pnpm tsx src/db/seed/index.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "lint": "eslint \"**/*.ts*\" --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.995.0",
    "@aws-sdk/s3-request-presigner": "3.995.0",
    "@dotenvx/dotenvx": "1.52.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.70.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.212.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.212.0",
    "@opentelemetry/resources": "2.5.1",
    "@opentelemetry/sdk-metrics": "2.5.1",
    "@opentelemetry/sdk-node": "0.212.0",
    "@opentelemetry/sdk-trace-node": "2.5.1",
    "@opentelemetry/semantic-conventions": "1.39.0",
    "@sentry/nextjs": "10.39.0",
    "@sentry/opentelemetry": "10.39.0",
    "@telli/ai-core": "workspace:*",
    "@t3-oss/env-core": "0.13.10",
    "drizzle-kit": "0.31.9",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "nanoid": "5.1.6",
    "next": "16.1.6",
    "pg": "8.18.0",
    "unstorage": "1.17.4",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "10.0.1",
    "@telli/typescript-config": "workspace:*",
    "@types/node": "24.10.13",
    "@types/pg": "8.16.0",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-turbo": "2.8.10",
    "eslint-plugin-prettier": "5.5.5",
    "prettier": "3.8.1",
    "prettier-eslint": "16.4.2",
    "typescript": "5.9.3",
    "tsx": "4.21.0",
    "vitest": "4.0.18"
  }
}
